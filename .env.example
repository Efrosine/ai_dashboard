# Environment variables for the project
# This file works for both local development and Docker

# Database Configuration
MYSQL_ROOT_PASSWORD=rootpassword123
MYSQL_DATABASE=ai_dashboard
MYSQL_USER=dashboard_user
MYSQL_PASSWORD=dashboard_pass123

# Database Connection (Docker will override DB_HOST via environment)
DB_HOST=localhost
DB_PORT=3306

# Server Configuration
NODE_ENV=development
API_PORT=3000
WEBSOCKET_PORT=8080

# Port Mappings (for Docker external access)
MYSQL_EXTERNAL_PORT=3306
SERVER_EXTERNAL_PORT=3000
WEBSOCKET_EXTERNAL_PORT=8080
FRONTEND_EXTERNAL_PORT=80
PHPMYADMIN_EXTERNAL_PORT=8081

# External API Configuration (to be configured later)
GEMINI_API_KEY=your_gemini_api_key_here
N8N_WEBHOOK_URL=your_n8n_webhook_url_here

# Development Settings
LOG_LEVEL=debug
ENABLE_CORS=true
